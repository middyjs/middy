{
	"name": "middy.js.org",
	"description": "SvelteKit SSR",
	"private": true,
	"version": "7.1.1",
	"type": "module",
	"scripts": {
		"start": "vite dev",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"build": "vite build",
		"build:scripts": "./bin/build-scripts",
		"build:vite": "vite build",
		"release:login": "wrangler login --browser false",
		"releases:domain": "curl https://api.cloudflare.com/client/v4/accounts/${ACCOUNT_ID}/pages/projects/middy/domains -H 'Content-Type: application/json' -H 'Authorization: Bearer ${API_KEY_W_PAGES_EDIT}' -d '{\"name\": \"middy.js.org\"}'",
		"release:deploy:workers": "wrangler deploy --config wrangler.worker.jsonc",
		"release:deploy:pages": "wrangler pages deploy"
	},
	"dependencies": {
		"@plausible-analytics/tracker": "0.4.4",
		"@ungap/custom-elements": "1.3.0",
		"hast-util-to-string": "3.0.1",
		"mdast-util-to-string": "4.0.0",
		"uuid": "13.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^7.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"esbuild": "^0.27.0",
		"mdsvex": "^0.12.0",
		"svelte": "^5.0.0",
		"vite": "^7.0.0",
		"vite-plugin-llms": "^1.0.0",
		"vite-plugin-mkcert": "^1.0.0",
		"vite-plugin-sitemap": "^0.8.0",
		"vite-plugin-sri": "^0.0.2",
		"wrangler": "^4.0.0"
	},
	"overrides": {
		"@sveltejs/kit": {
			"cookie": "0.7.2"
		}
	}
}
