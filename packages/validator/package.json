{
  "name": "@middy/validator",
  "version": "3.0.0",
  "description": "Validator middleware for the middy framework",
  "type": "module",
  "engines": {
    "node": ">=14"
  },
  "engineStrict": true,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs",
      "types": "./index.d.ts"
    }
  },
  "types": "index.d.ts",
  "files": [
    "index.js",
    "index.cjs",
    "index.d.ts"
