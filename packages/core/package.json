{
	"name": "@middy/core",
	"version": "7.1.2",
	"description": "ðŸ›µ The stylish Node.js middleware engine for AWS Lambda (core package)",
	"type": "module",
	"engines": {
		"node": ">=22"
	},
	"engineStrict": true,
	"publishConfig": {
		"access": "public"
	},
	"module": "./index.js",
	"sideEffects": false,
	"exports": {
		".": {
			"import": {
				"types": "./index.d.ts",
				"default": "./index.js"
			},
			"require": {
				"default": "./index.js"
			}
		},
		"./Standard": {
			"import": {
				"types": "./executionModeStandard.d.ts",
				"default": "./executionModeStandard.js"
			},
			"require": {
				"default": "./executionModeStandard.js"
			}
		},
		"./DurableContext": {
			"import": {
				"types": "./executionModeDurableContext.d.ts",
				"default": "./executionModeDurableContext.js"
			},
			"require": {
				"default": "./executionModeDurableContext.js"
			}
		},
		"./StreamifyResponse": {
			"import": {
				"types": "./executionModeStreamifyResponse.d.ts",
				"default": "./executionModeStreamifyResponse.js"
			},
			"require": {
				"default": "./executionModeStreamifyResponse.js"
			}
		}
	},
	"types": "index.d.ts",
	"files": [
		"index.js",
		"executionModeStandard.js",
		"executionModeStandard.d.ts",
		"executionModeDurableContext.js",
		"executionModeDurableContext.d.ts",
		"executionModeStreamifyResponse.js",
		"executionModeStreamifyResponse.d.ts",
		"index.d.ts"
	],
	"scripts": {
		"test": "npm run test:unit && npm run test:fuzz",
		"test:unit": "node --test --test-force-exit",
		"test:fuzz": "node --test index.fuzz.js",
		"test:perf": "node --test index.perf.js",
		"test:profile": "node --prof __benchmarks__/index.js && node --prof-process --preprocess -j isolate*.log | speedscope -"
	},
	"license": "MIT",
	"keywords": [
		"Lambda",
		"Middleware",
		"Serverless",
		"Framework",
		"AWS",
		"AWS Lambda",
		"Function URL",
		"Durable function"
	],
	"author": {
		"name": "Middy contributors",
		"url": "https://github.com/middyjs/middy/graphs/contributors"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/middyjs/middy.git",
		"directory": "packages/core"
	},
	"bugs": {
		"url": "https://github.com/middyjs/middy/issues"
	},
	"homepage": "https://middy.js.org",
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/willfarrell"
	},
	"peerDependencies": {
		"@aws/durable-execution-sdk-js": "^1.0.0"
	},
	"peerDependenciesMeta": {
		"@aws/durable-execution-sdk-js": {
			"optional": true
		}
	},
	"devDependencies": {
		"@aws/durable-execution-sdk-js": "^1.0.0",
		"@aws/durable-execution-sdk-js-testing": "^1.0.0",
		"@datastream/core": "0.0.42",
		"@types/aws-lambda": "^8.0.0",
		"@types/node": "^22.0.0"
	},
	"gitHead": "7a6c0fbb8ab71d6a2171e678697de9f237568431"
}
